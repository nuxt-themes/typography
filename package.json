{
  "name": "@nuxt-themes/typography",
  "version": "0.1.3",
  "description": "A beautifuk typography with a set of prose components to use with Nuxt Content.",
  "keywords": [
    "nuxt",
    "nuxt-content",
    "nuxt-theme"
  ],
  "type": "module",
  "exports": {
    ".": "./nuxt.config.ts",
    "./app-config": "./app.config.ts",
    "./tokens": "./tokens.config.ts",
    "./css": "./main.css"
  },
  "files": [
    "components",
    "nuxt.config.ts",
    "nuxt.schema.ts",
    "tokens.config.ts",
    "main.css"
  ],
  "scripts": {
    "dev": "nuxt dev .demo",
    "build": "nuxi build .demo",
    "generate": "nuxi generate .demo",
    "preview": "nuxi preview .demo",
    "lint": "eslint --ext .ts,.js,.vue .",
    "prepublishOnly": "pnpm lint",
    "release": "release-it"
  },
  "dependencies": {
    "@nuxt-themes/tokens": "latest",
    "@nuxtjs/color-mode": "^3.2.0",
    "nuxt-config-schema": "^0.3.4",
    "nuxt-icon": "^0.1.8",
    "pinceau": "latest"
  },
  "devDependencies": {
    "@nuxt/content": "^2.2.2",
    "@nuxthq/studio": "^0.3.3",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.29.0",
    "nuxt": "^3.0.0",
    "nuxt-plausible": "^0.1.2",
    "release-it": "^15.5.1",
    "typescript": "^4.9.3"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss",
        "vue"
      ]
    }
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
